<nemea-supervisor xmlns="urn:ietf:params:xml:ns:yang:nemea-test-1">
  <module-group>
    <name>Data sources</name>
    <enabled>true</enabled>
    <module>
      <name>ipfixcol</name>
      <path>/usr/bin/ipfixcol</path>
      <enabled>true</enabled>
      <params>-c /home/jelen/Desktop/etc/ipfixcol/ipfixcol-startup.xml -p /tmp/ipfixcol.pid</params>
    </module>

		<module>
      <name>ipfixsend</name>
      <enabled>true</enabled>
      <path>/usr/bin/ipfixsend</path>
		  <params>-i /home/jelen/Desktop/data.ipfix -R 0.5</params>
    </module>
  </module-group>

  <module-group>
    <name>Detectors</name>
    <enabled>true</enabled>

		<module>
			 <name>vportscan_detector</name>
			 <enabled>true</enabled>
			 <path>/usr/local/bin/vportscan_detector</path>
			 <params>-vvv</params>
			 <interface>
				<id>1</id>
			  <type>UNIXSOCKET</type>
			  <direction>IN</direction>
				<in-params>
					<unix-params>
						<socket-name>flow_data_source</socket-name>
					</unix-params>
				</in-params>
			 </interface>
			 <interface>
				<id>2</id>
			  <type>UNIXSOCKET</type>
			  <direction>OUT</direction>
				<out-params>
					<unix-params>
						<socket-name>vportscan_out</socket-name>
					</unix-params>
				</out-params>
			 </interface>
		</module>
  </module-group>
  <module-group>
    <name>Reporters</name>
    <enabled>true</enabled>
  </module-group>
  <module-group>
    <name>Loggers</name>
    <enabled>true</enabled>
  </module-group>

	<module-group>
    <name>To remove</name>
    <enabled>true</enabled>
		<module>
			 <name>intable_module</name>
			 <enabled>true</enabled>
			 <path>/asdasd</path>
			 <last-pid>123</last-pid>
		</module>
  </module-group>

</nemea-supervisor>
