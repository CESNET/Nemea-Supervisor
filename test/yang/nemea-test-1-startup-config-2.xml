<nemea-supervisor xmlns="urn:ietf:params:xml:ns:yang:nemea-test-1">
	<module-group>
    <name>Test1</name>
    <enabled>true</enabled>
		<module>
			<name>intable_module</name>
			<path>/asdasd</path>
			<instance>
				<name>inst 1</name>
				<enabled>true</enabled>
				<max-restarts-per-min>4</max-restarts-per-min>
			  <last-pid>123</last-pid>
				<interface>
					<name>tcp-out</name>
					<type>TCP</type>
					<direction>OUT</direction>
					
						<timeout>HALF_WAIT</timeout>
						<buffer>on</buffer>
						<autoflush>off</autoflush>
						<tcp-params>
							<port>8989</port>
							<max-clients>2</max-clients>
						</tcp-params>
					
				</interface>

				<interface>
					<name>tcp-tls-out</name>
					<type>TCP-TLS</type>
					<direction>OUT</direction>
					
						<timeout>NO_WAIT</timeout>
						<buffer>off</buffer>
						<autoflush>123</autoflush>
						<tcp-tls-params>
							<port>3332</port>
							<keyfile>/a/b</keyfile>
							<certfile>/a/b/c</certfile>
							<cafile>/a/b/c/d</cafile>
						</tcp-tls-params>
					
				</interface>

				<interface>
					<name>unix-out</name>
					<type>UNIXSOCKET</type>
					<direction>OUT</direction>
					
						<timeout>12312</timeout>
						<autoflush>999</autoflush>
						<unix-params>
							<socket-name>socket-name</socket-name>
							<max-clients>333</max-clients>
						</unix-params>
					
				</interface>

				<interface>
					<name>file-out</name>
					<type>FILE</type>
					<direction>OUT</direction>
					
						<file-params>
							<name>filename</name>
							<mode>a</mode>
							<time>444</time>
							<size>555</size>
						</file-params>
					
				</interface>

				<interface>
					<name>bh-out</name>
					<type>BLACKHOLE</type>
					<direction>OUT</direction>
					
						<buffer>off</buffer>
					
				</interface>
			</instance>
		</module>

		<!-- same as intable_module above but without last-pid -->
		<module>
			<name>intable_module2</name>
			<path>/asdasd</path>
			<instance>
				<name>inst 1</name>
				<enabled>true</enabled>
				<max-restarts-per-min>4</max-restarts-per-min>
				<interface>
					<name>tcp-out</name>
					<type>TCP</type>
					<direction>OUT</direction>
						<timeout>HALF_WAIT</timeout>
						<buffer>on</buffer>
						<autoflush>off</autoflush>
						<tcp-params>
							<port>8989</port>
							<max-clients>2</max-clients>
						</tcp-params>
				</interface>

				<interface>
					<name>tcp-tls-out</name>
					<type>TCP-TLS</type>
					<direction>OUT</direction>
						<timeout>NO_WAIT</timeout>
						<buffer>off</buffer>
						<autoflush>123</autoflush>
						<tcp-tls-params>
							<port>3332</port>
							<keyfile>/a/b</keyfile>
							<certfile>/a/b/c</certfile>
							<cafile>/a/b/c/d</cafile>
						</tcp-tls-params>
				</interface>

				<interface>
					<name>unix-out</name>
					<type>UNIXSOCKET</type>
					<direction>OUT</direction>
						<timeout>12312</timeout>
						<autoflush>999</autoflush>
						<unix-params>
							<socket-name>socket-name</socket-name>
							<max-clients>333</max-clients>
						</unix-params>
				</interface>

				<interface>
					<name>file-out</name>
					<type>FILE</type>
					<direction>OUT</direction>
						<file-params>
							<name>filename</name>
							<mode>a</mode>
							<time>444</time>
							<size>555</size>
						</file-params>
				</interface>

				<interface>
					<name>bh-out</name>
					<type>BLACKHOLE</type>
					<direction>OUT</direction>
					<buffer>off</buffer>
				</interface>
			</instance>
		</module>

		<module>
			<name>intable_module3</name>
			<path>/ab</path>
			<instance>
				<name>inst 3-1</name>
				<enabled>true</enabled>
			</instance>
		</module>

		<module>
			<name>intable_module4</name>
			<path>/ac</path>
			<instance>
				<name>inst 4-1</name>
				<enabled>false</enabled>
			</instance>
		</module>
  </module-group>

	<module-group>
		<name>Test 2</name>
		<enabled>true</enabled>
	</module-group>
</nemea-supervisor>
