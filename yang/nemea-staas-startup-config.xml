<?xml version="1.0"?>
<nemea-supervisor xmlns="urn:ietf:params:xml:ns:yang:nemea">
  <module-group>
    <name>Data sources</name>
    <enabled>true</enabled>
    <module>
      <name>ipfixcol</name>
      <path>/usr/bin/ipfixcol</path>
      <instance>
				<name>ipfixcol1</name>
        <enabled>true</enabled>
        <params>-c /home/jelen/Desktop/etc/ipfixcol/ipfixcol-startup.xml -p /tmp/ipfixcol.pid</params>
      </instance>
    </module>
    <module>
      <name>ipfixsend</name>
      <path>/usr/bin/ipfixsend</path>
      <instance>
        <name>ipfixsend1</name>
        <enabled>true</enabled>
        <params>-i /home/jelen/Desktop/data.ipfix -R 0.5</params>
      </instance>
    </module>
  </module-group>
  <module-group>
    <name>Detectors</name>
    <enabled>true</enabled>
    <module>
      <name>vportscan_detector</name>
      <path>/usr/local/bin/vportscan_detector</path>
      <instance>
        <name>vportscan_detector1</name>
        <enabled>true</enabled>
        <params>-vvv</params>
        <interface>
          <name>if1</name>
          <type>UNIXSOCKET</type>
          <direction>IN</direction>
          <unix-params>
            <socket-name>flow_data_source</socket-name>
          </unix-params>
        </interface>
        <interface>
          <name>if2</name>
          <type>UNIXSOCKET</type>
          <direction>OUT</direction>
          <unix-params>
            <socket-name>vportscan_out</socket-name>
          </unix-params>
        </interface>
      </instance>
    </module>
  </module-group>
  <module-group>
    <name>Reporters</name>
    <enabled>true</enabled>
  </module-group>
  <module-group>
    <name>Loggers</name>
    <enabled>true</enabled>
  </module-group>
</nemea-supervisor>
