{
  "nemea:supervisor": {
    "available-module": [
      {
        "name": "ipfixcol",
        "path": "/usr/bin/ipfixcol",
        "description": "TODO",
        "use-trap-ifces": false,
        "trap-monitorable": true,
        "is-sysrepo-ready": false
      },
      {
        "name": "ipfixsend",
        "path": "/usr/bin/ipfixsend",
        "description": "TODO",
        "use-trap-ifces": false,
        "trap-monitorable": false,
        "is-sysrepo-ready": false
      },
      {
        "name": "vportscan_detector",
        "path": "/usr/bin/nemea/vportscan_detector",
        "description": "TODO",
        "use-trap-ifces": true,
        "trap-monitorable": true,
        "is-sysrepo-ready": false
      }
    ],
    "instance": [
      {
        "name": "ipfixcol1",
        "module-ref": "ipfixcol",
        "enabled": true,
        "params": "-c /data/ipfixcol-files/ipfixcol-startup.xml -p /tmp/ipfixcol.pid",
        "interface": [
          {
            "name": "if1",
            "type": "UNIXSOCKET",
            "direction": "OUT"
          },
          {
            "name": "if2",
            "type": "UNIXSOCKET",
            "direction": "OUT"
          }
        ]
      },
      {
        "name": "ipfixsend1",
        "module-ref": "ipfixsend",
        "enabled": true,
        "params": "-i /data/ipfixcol-files/data.ipfix -R 0.5"
      },
      {
        "name": "vportscan_detector1",
        "module-ref": "vportscan_detector",
        "enabled": true,
        "interface": [
          {
            "name": "if1",
            "type": "UNIXSOCKET",
            "direction": "IN",
            "unix-params": {
              "socket-name": "flow_data_source"
            }
          },
          {
            "name": "if2",
            "type": "UNIXSOCKET",
            "direction": "OUT",
            "unix-params": {
              "socket-name": "vportscan_out"
            }
          }
        ],
        "params": "-vvv"
      }
    ]
  }
}
